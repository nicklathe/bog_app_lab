<div class="outer col-md-6 col-md-offset-3">
  <div class="col-md-12 text-center">
    <h1>Edit: "<%= @tag.name %>"</h1>
  </div>
  <div class="col-md-6 col-md-offset-3">
    <%= bootstrap_form_for :tag do |f| %>
      <%= f.text_field :name, :placeholder => "Tag Name" %>
      <%= f.submit :class => "btn btn-default btn-new" %>
    <% end %>
  </div>
</div>